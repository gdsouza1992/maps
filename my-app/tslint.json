{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-over-type-literal": false,
    "curly": [true, "ignore-same-line"],
    "no-debugger": false,
    "no-console": false
  }
}
